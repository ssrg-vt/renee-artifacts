% 

wordFromInteger[A : TYPE](dict_Machine_word_Size_a : size_class[A])(i : int) : mword =  
  IF int_lt(i)((0)) THEN
   mword_neg(wordFromNatural[A](dict_Machine_word_Size_a)(int_abs(-(i)))) ELSE
   wordFromNatural[A](dict_Machine_word_Size_a)(int_abs(i)) ENDIF


